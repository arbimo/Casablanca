<search-backend>
  <name>A DBPedia search backend for cities</name>
  <end-point>
    <url>http://dbpedia.org/sparql</url>
  </end-point>
  <search>
    <match>
      <type>exact</type>
    </match>
    <search-predicate uri="http://xmlns.com/foaf/0.1/name" weight="25"/>
  </search>
  <popularity>
    <measure>
      <predicate>http://dbpedia.org/ontology/populationTotal</predicate>
      <max>1000000</max>
    </measure>
  </popularity>
</search-backend>