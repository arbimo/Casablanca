<?xml version="1.0" encoding="utf-8"?>
<profile>
  <name>Local Fuseki End Point for Yago2 for cities (population as score)</name>
  <end-point>
    <url>http://localhost:3030/Yago/query</url>
  </end-point>
  <search>
    <search-predicate>
      <uri>yago:hasPreferredName</uri>
      <weight>50</weight>
    </search-predicate>
    <search-predicate>
      <uri>rdfs:label</uri>
      <weight>25</weight>
    </search-predicate>
  </search>
  <popularity>
    <measure>
      <label>Incoming links popularity</label>
      <predicate>yago:hasNumberOfWikipediaLinks</predicate>
    </measure>
    <measure>
      <label>Population popluarity</label>
      <predicate>yago:hasPopulation</predicate>
    </measure>
  </popularity>
  <properties>
    <property>
      <label>Name</label>
      <predicate>yago:hasPreferredName</predicate>
    </property>
  </properties>
</profile>